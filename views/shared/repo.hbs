{{#each repos}}
<div class="row">
    <div class="panel">
        <div class="panel-heading">
            <a class="" href="{{html_url}}"><h3>{{name}} <span class="glyphicon glyphicon-paperclip" aria-hidden="true"></span></h3></a>
            <h4>{{description}}</h4>
        </div>
        <div class="panel-body">
            <span>{{created_at}}</span>
        </div>
    </div>
</div>
{{/each}}

{{#if nextUrl}}
<a  id="fetcher" data-component="repo-fetcher" data-limit="{{repoLimit}}" data-offset="{{repoOffset}}"></a>
{{/if}}